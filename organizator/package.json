{
  "name": "organizator",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test-no-cover": "jest --coverage=false",
    "test-watch": "jest --watchAll",
    "lint": "eslint **/*.tsx & eslint **/*.ts & eslint **/*-test.js --fix",
    "check-lint": "eslint **/*.tsx & eslint **/*.ts & eslint **/*-test.js",
    "format": "npx prettier --write .",
    "check-format": "npx prettier -c ."
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/app/+html.tsx",
      "!**/constants/**",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.2",
    "@types/react": "~18.2.14",
    "axios": "^1.6.2",
    "expo": "~49.0.15",
    "expo-constants": "~14.4.2",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-splash-screen": "~0.20.4",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-alert-notification": "^0.3.5",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-indicators": "^0.17.0",
    "react-native-progress": "^5.0.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-simple-dialogs": "^2.0.1",
    "react-native-toast-message": "^2.2.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.6",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "prettier": "^3.1.1",
    "styled-components": "5.2.1"
  },
  "private": true
}
